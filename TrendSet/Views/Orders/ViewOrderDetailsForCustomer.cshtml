@model IEnumerable<TrendSet.Models.Order>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table align="center" class="table table-striped" style="background-color:white">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TailorDressCategoryMapping.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TopMaterialType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TopLengths)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Neck)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TopWaist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShoulderLength)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BottomMaterialType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BottomLength)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hip)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KneeLength)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpectedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bill)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Courier)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BillStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TailorDressCategoryMapping.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TopMaterialType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TopLengths)
